{
    "name": "prawee/yii2-app-pheme",
    "description": "Yii 2 Modified Application Template",
    "keywords": ["yii2", "framework", "advanced", "application template"],
    "homepage": "http://phe.me/",
    "type": "project",
    "license": "MIT",
    "minimum-stability": "dev",
    "repositories": [
        {
            "type": "git",
            "url": "https://bitbucket.org/prawee/yii2-auth-module.git"
        },
        {
            "type": "git",
            "url": "https://github.com/prawee/yii2-grid.git"
        },
        {
            "type": "git",
            "url": "https://github.com/prawee/yii2-components.git"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "yiisoft/yii2": "*",
        "yiisoft/yii2-bootstrap": "*",
        "yiisoft/yii2-swiftmailer": "*"
    },
    "require-dev": {
        "yiisoft/yii2-codeception": "*",
        "yiisoft/yii2-debug": "*",
        "yiisoft/yii2-gii": "*",
        "prawee/yii2-auth-module": "*",
        "prawee/yii2-components":"*",
        "prawee/yii2-grid":"*",
        "kartik-v/yii2-grid": "*",
        "zhuravljov/yii2-datetime-widgets" : "dev-master",
        "kartik-v/yii2-date-range": "dev-master",
        "kartik-v/yii2-datecontrol": "dev-master"
    },
    "suggest": {
        "codeception/codeception": "Codeception, 1.8.*@dev is currently works well with Yii.",
        "codeception/specify": "BDD style code blocks for PHPUnit and Codeception",
        "codeception/verify": "BDD Assertions for PHPUnit and Codeception",
        "yiisoft/yii2-faker": "Fixtures generator for Yii2 based on Faker lib"
    },
    "scripts": {
        "post-create-project-cmd": [
            "yii\\composer\\Installer::setPermission"
        ]
    },
    "config": {
        "process-timeout": 1800
    },
    "extra": {
        "writable": [
            "admin/app/runtime",
            "admin/assets",
            "api/app/runtime",
            "app/runtime",
            "app/assets"
        ]
    }
}
